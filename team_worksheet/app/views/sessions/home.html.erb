
<!DOCTYPE html>
<html>

<head>
  <title>Team Worksheet</title>
    <%= stylesheet_link_tag "team" %>
</head>
<body>
<!-- Navigation -->
	    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
	        <div class="container">
	            <!-- Brand and toggle get grouped for better mobile display -->
	            <div class="navbar-header">

	                <%= link_to "Team Worksheet", root_url, class: "navbar-brand" %>
	            </div>
	            <!-- Collect the nav links, forms, and other content for toggling -->
	            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
	                <ul class="nav navbar-nav">
	                    <div id="worksheet">
                            <%= simple_form_for @user, url: users_new_path(@user), html: { method: :put} do |f| %>
                            <h1>
                            <%= f.input_field :username, label: 'give me number', as: :integer %>
                            </h1>
                            <br>
                            <%= f.input_field :salt, as: :integer %>
                            <br>
                            <%= f.button :submit, url: sessions_home_url %>
                            <% end %>
                          <input name="name" type="hidden" value="0">
                          <input name="poop" type="hidden" value="1">
                      </div>
	                </ul>
	            </div>
	            <!-- /.navbar-collapse -->
	        </div>
	        <!-- /.container -->
	    </nav>

	    <!-- Page Content -->
	    <div class="container">

	    	<div class="row">
		    	<div class="col-lg-12 text-center">
		    	</div>
	    	</div>
	    	<!-- /.row -->
	    </div>
	    <!-- /.container -->
	  <style>body{padding-top:70px;}</style>
	</body>
	</html>
